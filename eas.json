{
  "cli": {
    "version": ">= 13.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {},
      "android": {},
      "environment": "development",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "credentialsSource": "local"
      },
      "android": {
        "buildType": "apk",
        "credentialsSource": "local"
      },
      "environment": "preview",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "credentialsSource": "local"
      },
      "android": {
        "buildType": "apk",
        "credentialsSource": "local"
      },
      "environment": "production",
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
